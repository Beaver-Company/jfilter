<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE config PUBLIC
        "-//json/json-ignore mapping DTD 1.0//EN"
        "https://rkonovalov.github.io/json-ignore-schema-1.0.dtd">
<config>
    <controller class-name="com.json.ignore.mock.webservice.WSMethod">
        <strategy attribute-name="" attribute-value="">
            <filter class="com.json.ignore.mock.MockUser">
                <field name="id"/>
                <field name="password"/>
                <field name="email"/>
            </filter>
        </strategy>
    </controller>
</config>